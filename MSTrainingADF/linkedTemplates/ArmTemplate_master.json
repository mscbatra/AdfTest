{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "MSTrainingADF"
		},
		"AzureSqlConnectionInfo_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureSqlConnectionInfo'"
		},
		"AzureSqlLinkedService_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureSqlLinkedService'"
		},
		"DataLakeConnInfo_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'DataLakeConnInfo'"
		},
		"DataLakeLinkedService_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'DataLakeLinkedService'"
		},
		"DataLakeConnInfo_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://mstrainingdatalake.dfs.core.windows.net/"
		},
		"DataLakeLinkedService_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://mstrainingdatalake.dfs.core.windows.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "MSTrainingADF_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureSqlConnectionInfo_connectionString": {
						"value": "[parameters('AzureSqlConnectionInfo_connectionString')]"
					},
					"AzureSqlLinkedService_connectionString": {
						"value": "[parameters('AzureSqlLinkedService_connectionString')]"
					},
					"DataLakeConnInfo_accountKey": {
						"value": "[parameters('DataLakeConnInfo_accountKey')]"
					},
					"DataLakeLinkedService_accountKey": {
						"value": "[parameters('DataLakeLinkedService_accountKey')]"
					},
					"DataLakeConnInfo_properties_typeProperties_url": {
						"value": "[parameters('DataLakeConnInfo_properties_typeProperties_url')]"
					},
					"DataLakeLinkedService_properties_typeProperties_url": {
						"value": "[parameters('DataLakeLinkedService_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}